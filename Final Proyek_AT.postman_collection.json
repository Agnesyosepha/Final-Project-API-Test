					"name": "Kota Valid",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"pm.test(\"Verify response structure and values\", function () {\r",
									"    const response = pm.response.json();\r",
									"\r",
									"    // Status code\r",
									"    pm.response.to.have.status(200);\r",
									"\r",
									"    // Properti utama\r",
									"    pm.expect(response).to.have.property(\"coord\");\r",
									"    pm.expect(response).to.have.property(\"weather\");\r",
									"    pm.expect(response).to.have.property(\"main\");\r",
									"    pm.expect(response).to.have.property(\"wind\");\r",
									"    pm.expect(response.name).to.eql(\"Medan\");\r",
									"\r",
									"    // Nilai tertentu\r",
									"    pm.expect(response.sys.country).to.eql(\"ID\");\r",
									"    pm.expect(response.main.temp).to.be.within(273.15, 373.15);\r",
									"\r",
									"    // Struktur array weather\r",
									"    pm.expect(response.weather).to.be.an(\"array\").that.is.not.empty;\r",
									"    pm.expect(response.weather[0]).to.have.property(\"id\");\r",
									"    pm.expect(response.weather[0]).to.have.property(\"main\");\r",
									"    pm.expect(response.weather[0]).to.have.property(\"description\");\r",
									"\r",
									"    // Properti opsional\r",
									"    if (response.rain) {\r",
									"        pm.expect(response.rain).to.have.property(\"1h\");\r",
									"        pm.expect(response.rain[\"1h\"]).to.be.a(\"number\");\r",
									"    }\r",
									"\r",
									"    // Tipe data\r",
									"    pm.expect(response.main.temp).to.be.a(\"number\");\r",
									"    pm.expect(response.name).to.be.a(\"string\");\r",
									"\r",
									"    // Waktu\r",
									"    pm.expect(response.sys.sunrise).to.be.a(\"number\");\r",
									"    pm.expect(response.sys.sunset).to.be.a(\"number\");\r",
									"    pm.expect(response.sys.sunrise).to.be.below(response.sys.sunset);\r",
									"});\r",
									"\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
